{
  "rules": {
    ".read": true,
    "repos": {
      "$entity": {
        "$repo": {
          "builds": {
            ".indexOn": ["createdAt"]
          }
        }
      }
    },
    "users": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    }
  }
}
