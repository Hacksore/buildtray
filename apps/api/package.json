{
  "name": "api",
  "scripts": {
    "start": "concurrently npm:build:watch npm:server",
    "build:watch": "rollup -w -c rollup.config.js",
    "server": "firebase emulators:start --import=seed",
    "build": "rimraf ./dist/**/* && rollup -c rollup.config.js",
    "deploy": "firebase deploy --only functions"
  },
  "engines": {
    "node": "16"
  },
  "type": "module",
  "main": "dist/index.js",
  "dependencies": {
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.19.0",
    "got": "^12.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "private": true
}
